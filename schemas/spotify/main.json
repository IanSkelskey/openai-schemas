{
  "openapi": "3.0.0",
  "info": {
    "title": "Spotify API",
    "description": "A simple API to get information about a Spotify user",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://api.spotify.com/v1",
      "description": "Spotify API"
    }
  ],
  "paths": {
    "/audio-features": "{{ audio-features }}",
    "/audio-features/{track_id}": "{{ audio-features/track_id }}",
    "/me/playlists": "{{ me/playlists }}",
    "/playlists/{playlist_id}": "{{ playlists/playlist_id }}",
    "/playlists/{playlist_id}/images": "{{ playlists/playlist_id/images }}",
    "/playlists/{playlist_id}/tracks": "{{ playlists/playlist_id/tracks }}",
    "/recommendations": "{{ recommendations }}",
    "/search": "{{ search }}",
    "/tracks/{track_id}": "{{ tracks/track_id }}",
    "/users/{user_id}/playlists": "{{ users/user_id/playlists }}"
  }
}