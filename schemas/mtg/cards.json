{
	"operationId": "getCards",
	"summary": "Retrieve a list of cards",
	"responses": {
	  "200": {
		"description": "A list of cards",
		"content": {
		  "application/json": {
			"schema": {
			  "type": "object",
			  "properties": {
				"cards": {
				  "type": "array",
				  "items": { "$ref": "./card_details.json" }
				}
			  }
			}
		  }
		}
	  }
	}
  }
  