{
  "openapi": "3.1.0",
  "info": {
    "title": "GitHub Repository Details",
    "description": "Fetches details of a specified GitHub repository",
    "version": "1.1.0"
  },
  "servers": [
    {
      "url": "https://api.github.com/",
      "description": "GitHub API"
    }
  ],
  "paths": {
    "/repos/{owner}/{repo}": "{{ repos/owner/repo }}",
    "/repos/{owner}/{repo}/contents/{path}": "{{ repos/owner/repo/contents/path }}",
    "/repos/{owner}/{repo}/contents/{path}/branch": "{{ repos/owner/repo/contents/branch }}",
    "/repos/{owner}/{repo}/metadata/{path}": "{{ repos/owner/repo/metadata }}",
    "/repos/{owner}/{repo}/branches": "{{ repos/owner/repo/branches }}"
  }
}
